{
    "manifest_version": 3,
    "name": "Selective Video Mute",
    "version": "1.0",
    "description": "Mute videos based on tags or keywords.",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
}
  